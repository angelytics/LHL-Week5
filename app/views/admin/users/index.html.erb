<h1>ROTTEN MANgoes</h1>

<div class="container">
  <div class="well">
    <% @users.each do |user| %>
      <% if current_user.email != user.email%>
        <h4>User Name: <%= link_to user.email,edit_admin_user_path(user) %></h4>
        <button><%= link_to "delete", admin_user_path(user), method: :delete, :'data-confirm'=>"You sure?"%></button>
        <button><%= link_to "preview", admin_user_preview_path(user.id), method: :post%></button>
      <% end %>
    <% end %>
</div>
</div>

